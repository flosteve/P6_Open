<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% include 'default/stylesheets.html.twig' %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body class="signup-page  ">
        {% block menu %}{% include 'default/menu.html.twig' %}{% endblock %}
        {% block body %}

        {% if app.request.hasPreviousSession %}
            {% for type, messages in app.session.flashbag.all() %}
                {% for message in messages %}
                    <div class="flash-{{ type }}">
                        {{ message }}
                    </div>
                {% endfor %}
            {% endfor %}
        {% endif %}

        <div>
            {% block fos_user_content %}
            {% endblock fos_user_content %}
        </div>


            {% endblock %}
        {% block footer %}{% include'default/footer.html.twig' %}{% endblock %}
        {% block javascripts %}{% include 'default/javascripts.html.twig' %}{% endblock %}
    </body>
</html>
